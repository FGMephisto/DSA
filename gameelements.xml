<root>
	<distance>
		<base>1</base>
		<diagmult>1</diagmult>
		<suffix>m</suffix>
	</distance>

	<imagesettings>
		<pointer defaultconeangle="5E" />
	</imagesettings>

	<hotkeybar>
		<title frame="hotkeytitlebox" frameoffset="2,-2,5,2" margin="2,7" font="hotkey" />
	</hotkeybar>

	<!-- Dice -->
	<panel name="dicepanel" merge="join">
		<anchored>
			<left offset="320" />
		</anchored>
	</panel>

	<die name="dF" merge="delete" />
	<die name="rF" merge="delete" />
	<die name="gF" merge="delete" />
	<die name="bF" merge="delete" />
	<die name="pF" merge="delete" />
	<die name="d4" merge="delete" />
	<die name="d8" merge="delete" />
	<die name="d10" merge="delete" />
	<die name="d12" merge="delete" />
	<die name="d100" merge="delete" />
</root>